<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tiyanrcode.guestbookclient" >

    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_LOGS"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".controller.LoginController"
            android:label="@string/activity_login" >
        </activity>
        <activity
            android:name=".controller.BookIdController">
            android:label="@string/activity_view" >
            </activity>
        <activity
            android:name=".controller.ViewController"
            android:label="@string/activity_view" >
        </activity>
        <activity
            android:name=".controller.AboutController" >
        </activity>
        <activity
            android:name=".controller.FamilyController" >
        </activity>
        <activity
            android:name=".controller.SearchController" >
        </activity>
    </application>

</manifest>
